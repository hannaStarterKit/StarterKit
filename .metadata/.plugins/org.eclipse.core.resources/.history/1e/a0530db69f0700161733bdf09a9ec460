	public int getWhichFrame() {
		return whichFrame;
	}

	/**
	 * @param whichFrame the whichFrame to set
	 */
	public void setWhichFrame(int whichFrame) { //nie lepiej to usunac i w konstruktorze po prostu wstawiac
